{
  "expo": {
    "name": "raavitoapp",
    "slug": "raavitoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "raavitoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "@/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rajanverma1987.raavitoapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rajanverma1987.raavitoapp"
    },
    "web": {
      "output": "single"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/foodthali.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "36d99e05-0e06-4848-ba0c-8ad9c0b3dd8b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/36d99e05-0e06-4848-ba0c-8ad9c0b3dd8b"
    }
  }
}
